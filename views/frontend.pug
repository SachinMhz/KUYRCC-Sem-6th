extends layout

block content
  // Header Image
  div.container
    img.img-fluid.pt-4(src='KUYRCC.png', alt='KUYRCC logo')
  // Navigation
  nav.container.navbar.navbar-expand-md.navbar-light.bg-light.sticky-top
    div.container-fluid
      a.navbar-brand(href='#')
        img.img-fluid(src='kuyrccText.png')
      button.navbar-toggler(type='button', data-toggle='collapse', data-target='#navbarResponsive')
        span.navbar-toggler-icon
      #navbarResponsive.collapse.navbar-collapse
        ul.navbar-nav.mr-auto
          li.nav-item.active
            a.nav-link(href='#')  Homme 
          li.nav-item
            a.nav-link(href='#')  Service 
          li.nav-item
            a.nav-link(href='#')  About 
          li.nav-item
            a.nav-link(href='#')  Contact 
      button.btn.btn-outline-info.waves-effect.ml-0.float-md-right(type='button',data-toggle="collapse", data-target="#Login") Log In
      a(href='KUYRCC/SignUp/')
        button.btn.btn-outline-info.waves-effect.mr-0.flaot-md-right(type='button') Sign Up
  
  // -sliding login and signup forms
  #Login.collapse.container.my-4
    .container-fluid.padding
      .row.text-center
        form.form-inline
          label(for='email') Email address:
          input.form-control(type='email')
          label(for='pwd') Password:
          input.form-control(type='password')
          .form-check
            label.form-check-label
              input.form-check-input(type='checkbox')
              |  Remember me
          button.btn.btn-primary(type='submit') Log In
          
  // - Image Slider
  #slides.container.carousel.slide(data-ride='carousel')
    ul.carousel-indicators
      li.active(data-target='#slides', data-slide-to='0')
      li(data-target='#slides', data-slide-to='1')
      li(data-target='#slides', data-slide-to='2')
    div.carousel-inner
      div.carousel-item.active
        img(src='red.jpg')
      div.carousel-item
        img(src='red2.jpg')
      div.carousel-item
        img(src='red3.jpg')
    a.carousel-control-prev(href='#slides', data-slide='prev')
      span.carousel-control-prev-icon
    a.carousel-control-next(href='#slides', data-slide='next')
      span.carousel-control-next-icon
